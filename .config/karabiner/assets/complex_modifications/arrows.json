{
  "title": "arrows",
  "rules": [{
    "description": "arrows: CMD+up -> page_up",
    "manipulators": [{
      "type": "basic",
      "from": {
        "modifiers": {
          "mandatory": ["left_command"],
          "optional": ["any"]
        },
        "key_code": "up_arrow"
      },
      "to": [{
        "key_code": "page_up"
      }]
    }]
  }, {
    "description": "arrows: CMD+down -> page_down",
    "manipulators": [{
      "type": "basic",
      "from": {
        "modifiers": {
          "mandatory": ["left_command"],
          "optional": ["any"]
        },
        "key_code": "down_arrow"
      },
      "to": [{
        "key_code": "page_down"
      }]
    }]
  }]
}
