{
  "title": "chrome",
  "rules": [{
    "description": "chrome: ctrl+command+pageup -> CMD+ctrl+left",
    "manipulators": [{
      "type": "basic",
      "from": {
        "modifiers": {
          "mandatory": ["left_control", "left_command"],
          "optional": ["any"]
        },
        "key_code": "page_up"
      },
      "conditions": [{
        "type": "frontmost_application_if",
        "bundle_identifiers": ["^com\\.google\\.Chrome$"]
      }],
      "to": [{
        "key_code": "left_arrow",
        "modifiers": [
          "left_command",
          "left_control"
        ]
      }]
    }]
  }, {
    "description": "chrome: ctrl+command+pagedown -> CMD+ctrl+right",
