{
  "title": "iterm2",
  "rules": [{
    "description": "iterm2: CMD+w -> ctrl+w",
    "manipulators": [{
      "type": "basic",
      "from": {
        "key_code": "w",
        "modifiers": {
          "mandatory": ["left_command"],
          "optional": ["any"]
        }
      },
      "conditions": [{
        "type": "frontmost_application_if",
        "bundle_identifiers": ["^com\\.googlecode\\.iterm2$"]
      }],
      "to": [
        {
          "key_code": "w",
          "modifiers": ["left_control"]
        }
      ]
    }]
  }, {
